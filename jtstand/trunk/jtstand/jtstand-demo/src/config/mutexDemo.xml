<testSequence xmlns="http://www.jtstand.com/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="DemoRandom" xsi:schemaLocation="http://www.jtstand.com/ http://www.jtstand.com/jtstand-1.0.xsd">
    <property name="DECIMAL_FORMAT" value="0.000"/>
    <mainStep name="RANDOM">
        <step name="not mutex parallel">
            <property name="VAR1" final="false"/>
            <step name="group" parallel="true">
                <step name="sub1">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
                <step name="sub2">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
                <step name="sub3">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
                <step name="sub4">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
            </step>
        </step>
        <step name="mutex parallel">
            <property name="VAR1" final="false" mutex="true"/>
            <step name="group" parallel="true">
                <step name="sub1">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
                <step name="sub2">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
                <step name="sub3">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
                <step name="sub4">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
            </step>
        </step>
        <step name="mutex sequential">
            <property name="VAR1" final="false" mutex="true"/>
            <step name="group">
                <step name="sub1">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
                <step name="sub2">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
                <step name="sub3">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
                <step name="sub4">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
            </step>
        </step>
        <step name="mutex sequential and parallel with eval">
            <property name="VAR1" final="false" mutex="true">(Integer) 100</property>
            <step name="group1">
                <step name="sub1">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
                <step name="group2" parallel="true">
                    <step name="sub2">
                        <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                    </step>
                    <step name="sub3">
                        <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                    </step>
                </step>
                <step name="sub4">
                    <stepFile subversionUrl="mutexSub.xml" revision="344"/>
                </step>
            </step>
        </step>
    </mainStep>
</testSequence>
